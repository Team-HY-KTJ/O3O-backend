{
    "env": {
      "browser": false,
      "node": true,
      "es2021": true
    },
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-console": "warn",
      "consistent-return": "off"
    },
    "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  }
  }
  